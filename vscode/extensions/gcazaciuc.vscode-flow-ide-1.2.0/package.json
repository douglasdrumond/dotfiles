{
	"name": "vscode-flow-ide",
	"displayName": "vscode-flow-ide",
	"description": "Visual studio code Flowtype support",
	"version": "1.2.0",
	"publisher": "gcazaciuc",
	"engines": {
		"vscode": "^1.6.0"
	},
	"categories": [
		"Languages",
		"Linters"
	],
	"activationEvents": [
		"onLanguage:javascriptreact",
		"onLanguage:javascript"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "flow.coverage",
				"title": "Flow-IDE: Toggle Flowtype coverage"
			},
			{
				"command": "flow.path",
				"title": "Flow-IDE: Show path to Flow"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Flow-IDE Configurations",
			"properties": {
				"flowide.enabled": {
					"type": "boolean",
					"default": true,
					"description": "Is flow enabled"
				},
				"flowide.useCodeSnippetsOnFunctionSuggest": {
					"type": "boolean",
					"default": true,
					"description": "Whether should autocomplete params name"
				},
				"flowide.pathToFlow": {
					"type": "string",
					"default": "",
					"description": "Path to flow binary"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"dependencies": {
		"cross-spawn": "^4.0.2",
		"js-beautify": "^1.6.4"
	},
	"icon": "flow-logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gcazaciuc/vscode-flow-ide"
	},
	"bugs": {
		"url": "https://github.com/gcazaciuc/vscode-flow-ide/issues"
	},
	"__metadata": {
		"id": "e607d339-50b5-4b33-85ac-1d79edc0c7b3",
		"publisherId": "7593a93b-2a67-4a85-90ec-87dc694b3353",
		"publisherDisplayName": "gcazaciuc"
	}
}